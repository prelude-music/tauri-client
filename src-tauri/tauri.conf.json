{
	"build": {
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "Prelude",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			}
		},
		"windows": [
			{
				"center": true,
				"title": "Prelude",
				"width": 1280,
				"height": 720
			}
		],
		"security": {
			"csp": null
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.github.prelude-app.tauri-client",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		}
	}
}